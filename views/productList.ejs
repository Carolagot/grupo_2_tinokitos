<%- include("./partials/head") %>

    <link rel="stylesheet" href="/css/styles.css">
    <title>Tinokitos Home</title>
</head>

<body>

    <%- include("./partials/header") %>

    <main>

        <img class="banner-desktop" src="/imagenes/banner.jpg" alt="">

        <img class="banner-cel" src="/imagenes/banner-cel.jpg" alt="">
        <br>
        <section>

            <div class="productos-destacados-desktop"><h1>Productos</h1></div>
            <div class="contenedor-productos-desktop">
                <article class="ventas-desktop">
                <img src="/imagenes/plato-recortada.jpg" alt="plato de silicona">
                <div class="texto">
                    <h2>Plato de Silicona</h2>
                    <h3>$3.200</h3>
                </div>
                </article>

                <article class="ventas-desktop">
                    <img src="/imagenes/bata-recortada.jpg" alt="bata">
                    <div class="texto">
                        <h2>Bata con Pantufla</h2>
                        <h3>$7.900</h3>
                    </div>
                </article>

                <article class="ventas-desktop">
                    <img  src="/imagenes/silla-recortada.jpg" alt="silla de comer">
                    <div class="texto">
                        <h2>Silla de Comer</h2>
                        <h3>$25.000</h3>
                </article>

                <article class="ventas-desktop">
                    <img src="/imagenes/bebe-recortada.jpg" alt="baberos">
                    <div class="texto">
                        <h2>Abrigo Osito</h2>
                        <h3>$6900</h3>
                </article>

            </div>

        
            <div class="contenedor-productos-desktop">
                <article class="ventas-desktop">
                <img src="/imagenes/coche-moises.jpg" alt="coche">
                <div class="texto">
                    <h2>Cochecito via</h2>
                    <h3>$55.500</h3>
                </div>
                </article>

                <article class="ventas-desktop">
                    <img src="/imagenes/baberos-recortada.jpg" alt="huevito">
                    <div class="texto">
                        <h2>Babero de Silicona</h2>
                        <h3>$2.200</h3>
                    </div>
                </article>

                <article class="ventas-desktop">
                    <img  src="/imagenes/nidito.jpg" alt="Nido de contención">
                    <div class="texto">
                        <h2>Nido de Contención</h2>
                        <h3>$25.000</h3>
                </article>

                <article class="ventas-desktop">
                    <img src="/imagenes/huevito.jpg" alt="baberos">
                    <div class="texto">
                        <h2>Huevito de auto</h2>
                        <h3>$21.600</h3>
                </article>

            </div>
        </section>

    </main>

    <%-include("./partials/footer")%>

</body>



</html>